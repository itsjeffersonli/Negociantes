@model IEnumerable<Negociantes.Models.Post>

@{
    ViewData["Title"] = "AnyAccount";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>AnyAccount</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
        </tr>
}
    </tbody>
</table>
